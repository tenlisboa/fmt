{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch CLI",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/cli/main.go",
      "cwd": "${workspaceFolder}",
      "args": ["${input:args}"]
    }
  ],
  "inputs": [
    {
      "id": "args",
      "type": "promptString",
      "description": "Enter arguments for the CLI",
      "default": ""
    }
  ]
}
